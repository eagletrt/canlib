                "CELLBOARD0",
                "CELLBOARD1",
                "CELLBOARD2",
                "CELLBOARD3",
                "CELLBOARD4",
                "CELLBOARD5"
            ],
            "description": "Set discharging status for cells",
            "contents": {
                "board_index": "uint8",
                "cells": "BalancingCells"
            }
        },
        {
            "name": "FW_UPDATE",
            "topic": "FW_UPDATE",
            "priority": 6,
            "sending": ["MAINBOARD"],
            "receiving": [
                "CELLBOARD0",
                "CELLBOARD1",
                "CELLBOARD2",
                "CELLBOARD3",
                "CELLBOARD4",
                "CELLBOARD5"
