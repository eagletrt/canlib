                "CELLBOARD1",
                "CELLBOARD2",
                "CELLBOARD3",
                "CELLBOARD4",
                "CELLBOARD5"
            ],
            "receiving": ["MAINBOARD"],
            "description": "Cells' temperatures",
            "contents": {
                "start_index": "uint8",
                "temp0": "uint8",
                "temp1": "uint8",
                "temp2": "uint8",
                "temp3": "uint8",
                "temp4": "uint8",
                "temp5": "uint8"
            }
        },
        {
            "name": "VOLTAGES",
            "topic": "VOLTAGE_INFO",
            "priority": 5,
            "sending": [
                "CELLBOARD0",
                "CELLBOARD1",
                "CELLBOARD2",
                "CELLBOARD3",
                "CELLBOARD4",
                "CELLBOARD5"
